{
  "manifest_version": 2,
  "name": "glTF Preview for GitHub",
  "version": "1.0",
  "description": "Adds preview button for glTF 2.0 models on GitHub.",
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
     "webNavigation"
  ]
}
